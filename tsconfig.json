{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/index-kai.js",
    "pages/index-kai.js",
    "lib/post.js",
    "lib/post.js",
    "node_modules/@types/node/matter.d.ts",
    "node_modules/@types/node/matter.d.ts",
    "node_modules/matter/src/matter.d.ts",
    "node_modules/matter/src/matter.d.ts"
  ],
  "exclude": ["node_modules"]
}
